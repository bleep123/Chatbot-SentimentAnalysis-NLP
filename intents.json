{"intents" : [
    {"tag": "greeting",
     "patterns": ["Hi there", "How are you", "Is anyone there?","Hey","Hola", "Hello", "Good day", "Hi", "Hey there"],
     "responses": ["Hello", "Good to see you again", "Hi there, how can I help?"],
     "context": [""]
    },
    {"tag": "goodbye",
     "patterns": ["Bye", "See you later", "Goodbye", "Nice chatting to you, bye", "Till next time"],
     "responses": ["See you!", "Have a nice day", "Bye! Come back again soon."],
     "context": [""]
    },
    {"tag": "thanks",
     "patterns": ["Thanks", "Thank you", "That's helpful", "Awesome, thanks", "Thanks for helping me"],
     "responses": ["My pleasure", "You're Welcome"],
     "context": [""]
    },
    {"tag": "query",
     "patterns": ["Tell me about yourself?", "Who are you?"],
     "responses": ["I am a chatbot for assisting your queries ", "I am your chatbot friend, ChatterEase"],
     "context": [""]
    },
    {"tag": "weather",
     "patterns": ["What's the weather like today?", "How is the weather?", "Can you tell me the weather?"],
     "responses": ["Sure, I can check that for you. What's the city name?"],
     "context": [""]
    },
    {"tag": "time",
     "patterns": ["What's the time?", "Do you know the time?"],
     "responses": ["Sure, the current time is {time}"],
     "context": [""]
    },
    {"tag": "date",
     "patterns": ["What's the date today?", "Can you tell me the date?"],
     "responses": ["Today's date is {date}"],
     "context": [""]
    },
    {"tag": "joke",
     "patterns": ["Tell me a joke", "Can you tell me a joke?"],
     "responses": ["Sure, here's a joke for you: {joke}"],
     "context": [""]
    },
    {"tag": "news",
     "patterns": ["What's the news?", "Can you give me the latest news?"],
     "responses": ["Sure, here's the latest news: {news}"],
     "context": [""]
    }
]
}